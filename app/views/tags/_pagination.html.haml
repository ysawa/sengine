%nav.pagination.next_tag
  - name = t('actions.see_more')
  - unless defined?(pagination_params)
    - pagination_params = {}
  = link_to_next_page tags, name, remote: true, params: pagination_params, class: 'btn btn-block btn-large see_more' do
    %span.see_more.btn.btn-block.btn-large.disabled= name
