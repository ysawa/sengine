<%- game_contents = '' %>
<%- @games.each do |game| %>
  <%- game_contents += render 'games/list', game: game %>
<%- end %>

$('#games').append('<%= escape_javascript(game_contents.html_safe) %>');

<%- games_pagination = render 'games/pagination', games: @games %>
$('nav.next_game').replaceWith('<%= escape_javascript(games_pagination) %>');
