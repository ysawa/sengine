$('.board_container').html('<%= escape_javascript(render 'boards/board', game: @game, board: @game.boards.last) %>');

<%- if current_user == @game.sente_user and @game.movements.last.gote? %>
  $.play_audio('put');
<%- elsif current_user == @game.gote_user and @game.movements.last.sente? %>
  $.play_audio('put');
<%- end %>
